@model List<Category>

<h1>Category List</h1>
<table>
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>
                    @obj.CategoryName;
                </td>
                <td>
                    @obj.DisplayOrder;
                </td>
            </tr>
        }
    </tbody>
</table>